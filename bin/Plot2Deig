FirstColor <-$FirstColor;
if (shape_level < 6 ){
ggplot(data = PCA, mapping = aes(x = $head[$ShiftC1], y = $head[$ShiftC2],shape =$shape)) + geom_point(size = $PointSize, alpha = $fillopacity,color=FirstColor ) +ggtitle("$title")  + theme(plot.title = element_text(hjust = 0.5))+labs(x="$Xlab_XX",y="$Ylab_YY")
} else {
ggplot(data = PCA, mapping = aes(x = $head[$ShiftC1], y = $head[$ShiftC2],shape =$shape)) + geom_point(size = $PointSize ,alpha = $fillopacity,color=FirstColor) +ggtitle("$title")  +theme(plot.title = element_text(hjust = 0.5))+  scale_shape_manual(values=seq(0,15))+labs(x="$Xlab_XX",y="$Ylab_YY") 	}

